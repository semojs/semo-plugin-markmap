{
  "name": "semo-plugin-markmap",
  "version": "2.0.0",
  "description": "A Semo plugin to convert markdown to mindmap, based on markmap.",
  "keywords": [
    "semo",
    "plugin",
    "markdown",
    "mindmap"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/semojs/semo-plugin-markmap"
  },
  "main": "lib/index.js",
  "type": "module",
  "types": "lib/index.d.ts",
  "license": "MIT",
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^22.15.14",
    "@typescript-eslint/eslint-plugin": "^8.32.0",
    "@typescript-eslint/parser": "^8.32.0",
    "@vitest/coverage-v8": "^3.1.3",
    "@vitest/eslint-plugin": "^1.1.44",
    "@vitest/ui": "^3.1.3",
    "eslint": "^9.26.0",
    "eslint-config-prettier": "^10.1.3",
    "globals": "^16.1.0",
    "husky": "^9.1.7",
    "jiti": "^2.4.2",
    "lint-staged": "^15.5.2",
    "prettier": "^3.5.3",
    "rimraf": "^6.0.1",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.32.0",
    "vitest": "^3.1.3"
  },
  "scripts": {
    "dev": "tsc -w",
    "build": "tsc",
    "clean": "rimraf lib tsconfig.tsbuildinfo",
    "test": "vitest run",
    "test:watch": "vitest ",
    "test:ui": "vitest --ui",
    "test:cov": "vitest run --coverage",
    "postversion": "git push && git push --tags",
    "prepublish": "npm run clean && npm run build",
    "prepare": "husky",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"**/*.{ts,js}\"",
    "format:check": "prettier --check \"**/*.{ts,js}\"",
    "deploy": "npm version patch",
    "deploy:minor": "npm version minor",
    "deploy:major": "npm version major"
  },
  "dependencies": {
    "@semo/core": "^2.0.10",
    "axios": "^1.9.0",
    "chokidar": "^4.0.3",
    "fs-extra": "^11.3.0",
    "get-stdin": "^9.0.0",
    "markmap-common": "^0.18.9",
    "markmap-lib": "^0.18.11",
    "markmap-render": "^0.18.10",
    "open": "^10.1.2",
    "semo-plugin-serve": "^2.0.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
